<table cellpadding="0" cellspacing="0" width="100%" style="font-family: Arial, sans-serif; font-size: 16px; background-color: #eeeeee;">
    <tr>
        <td style="padding: 10px 0 30px 0;">
            <table align="center" width="600" cellspacing="0" cellpadding="0" style="border-collapse: collapse;">
                <tr>
                    <td bgcolor="#ffffff" style="width: 100%;color: #000;padding: 10px 10px 10px 10px;">
                        <a href="#" style="color: rgba(0,0,0,.9);  padding-top: 5px; padding-bottom: 5px; margin-right: 16px;font-size: 20px;text-decoration: none;white-space: nowrap;">
                             <img src="<%-appicon%>" alt="app-logo" style="vertical-align: middle; margin-right: 5px"/> | <%-appname%>
                        <a/>
                    </td>
                </tr>
                <tr>
                    <td align="center" style="padding: 10px 10px 10px 10px; background-color: #F4F6F9;">
                        <table cellpadding="0" cellspacing="0" style="padding: 10px">
                            <tr>
                               <img src="<%-teamicon%>" />
                            </tr>
                            <tr>
                                <td style="padding: 10px"><b>Workspaname:</b> &nbsp;<%-workspaceName%>
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align: justify; padding: 10px">
                                    <b>Description:</b> &nbsp;<%-description%>
                                </td>
                            </tr>
                            <tr>
                                <td align="center" style="padding: 20px">
                                    <a href="<%-workspaceUrl%>"
                                        style="color: #ffffff;text-decoration: none;padding: 10px 12px; background-color: #58AF9B;border-radius: 8px">
                                        Join Workspace
                                    </a>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
                <tr>
                    <td bgcolor="#ffffff" align="center"
                        style="width: 100%;color: rgba(0,0,0,.9);padding: 10px 10px 10px 10px;">
                        <h3>&copy; <%-appname%>, <%-year%></h3>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>